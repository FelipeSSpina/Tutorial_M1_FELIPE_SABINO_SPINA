config_version=4

[application]

config/name="EXERCICIO_SEMANA7"
config/description="	Esse é um minijogo feito para a entrega da semana 7. É um jogo extremamente simples, em que o personagem pode se mover para direita, esquerda e pular, utilzando as setas do teclado ou utilizando D,A e tecla de espaço. O objetivo do jogo é passar pelos obstáculos e chegar ao ponto final para concluir o objetivo.
	"
run/main_scene="res://Tela_incial.tscn"
config/icon="res://Art/icon.png"
config/windows_native_icon="res://icon.ico"

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

